<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="stage_active" model="project.task.type">
            <field name="name">Active</field>
            <field name="sequence">0</field>
            <field name="project_ids" eval="[(6, 0, [ref('gpsi_crm.project_certification_sales_opportunities')])]"/>
        </record>

        <record id="stage_pending" model="project.task.type">
            <field name="name">Pending</field>
            <field name="sequence">1</field>
            <field name="project_ids" eval="[(6, 0, [ref('gpsi_crm.project_certification_sales_opportunities')])]"/>
        </record>

        <record id="stage_closed" model="project.task.type">
            <field name="name">Closed</field>
            <field name="sequence">2</field>
            <field name="fold">True</field>
        </record>

        <record id="stage_spam" model="project.task.type">
            <field name="name">Unassigned</field>
            <field name="sequence">3</field>
            <field name="fold">True</field>
        </record>

    </data>
</odoo>